//autor: Sebastian de la zerda

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main()

{
	int n;
	int numeros[100];
	int suma, i;
	suma=0;
	cout<<"ingrese la cantidad de numero de elementos: ";
	cin>>n;
	for (int i=0;i<n;i++)
	{
		cout<<"ingrese su numero: ";
	cin>> numeros[i];
	}
	for (int i=0;i<n;i++)
	{
		suma=suma+numeros[i];
	}
	cout<<"la suma de sus vectores es: "<<suma<<endl;

	getch();

}
