// factorial.cpp : Defines the entry point for the console application.
// Autor: Sebastian de la zerda

#include "stdafx.h"
#include "iostream"
#include <conio.h>

using namespace std;
void main()
{
	int i,n,factorial;
	cout<<"Ingrese el valor de n: ";
	cin>>n;
	factorial=1;
	i=1;
	while(i<=n)
	{
		factorial= factorial*i;
		i=i+1;
	}
cout<<"El factiorial es: "<<factorial;
getch();
}
