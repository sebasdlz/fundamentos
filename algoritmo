#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main()
{
	int n;
	int numeros[1000];
    int suma = 0;
	cout<<"Ingrese la cantidad de elementos que desee: "<<endl;
	cin>>n;
	for (int i=0; i<n; i++)
	{
		cout<<"Ingrese el numero: "<<endl;
		cin>>numeros[i];
		suma = suma + numeros[i];
	}
	   cout<<"La suma de los elementos del vector es es: "<<suma<<endl;

	   getch();
}
